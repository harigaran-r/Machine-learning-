      "source": [
        "\n",
        "\n",
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "from sklearn.svm import SVC\n",
        "\n",
        "\n",
        "np.random.seed(23022)\n",
        "X = np.random.uniform(-3, 3, 200).reshape(-1, 1)\n",
        "y = (X[:, 0]**2 + np.random.randn(200)*0.5 > 4).astype(int)\n",
        "\n",
        "\n",
        "X_poly = np.hstack([X, X**2])\n",
        "\n",
        "\n",
        "model = SVC(kernel='linear')\n",
        "model.fit(X_poly, y)\n",
        "\n",
        "\n",
        "plt.figure(figsize=(7,5))\n",
        "plt.scatter(X, y, c=y, cmap='coolwarm', s=40, edgecolors='k')\n",
        "plt.xlabel(\"X\")\n",
        "plt.ylabel(\"Label\")\n",
        "plt.title(\"Quadratic Data Classification using SVM\")\n",
        "\n",
        "\n",
        "x_vals = np.linspace(-3, 3, 200).reshape(-1,1)\n",
        "x_poly = np.hstack([x_vals, x_vals**2])\n",
        "y_pred = model.decision_function(x_poly)\n",
        "plt.plot(x_vals, (y_pred - y_pred.min())/(y_pred.max()-y_pred.min()), color='black', lw=2)\n",
        "plt.show()\n"
      ]
    }
  ]
}